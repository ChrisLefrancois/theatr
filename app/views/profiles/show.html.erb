<div class="container">
  <div class="header-box m-3">
    <img class="profile-avatar " style="height: 200px width: 200px" src="data:image/jpeg;base64," alt="">
    <div class="name-div">
      <h1 class="m-3"><%= @user.first_name %> <%= @user.last_name %></h1>
      <div class="info-box">
        <ul class="m-3">
          <p><strong>Email:</strong> <%= @user.email %></p>
          <p><strong>Username:</strong> <%= @user.user_name %></p>
        </ul>
      </div>
    </div>
  </div>

  <hr>
  <% @badges.each do |badge| %>
    <div class="profile-box">
      <p> <%= @user.first_name %> <%= badge.status == "completed" ? "Achieved this badge" : "Completed this badge" %></p>
      <p><%= badge.updated_at.strftime("%a %b %e at %l:%M %p") %></p>
      <% if current_user.teacher? || current_user == @user%>
        <%= link_to 'See More', results_path(@user, badge.activity), class: "btn btn-primary" %>
      <% end %>
    </div>
  <% end %>
</div>
